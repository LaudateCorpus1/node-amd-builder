{
    "name":"amd-builder",
    "description":"A service that builds bundles from AMD projects resources",
    "version":"0.2.4",
    "keywords":[
        "jquery mobile",
        "amd",
        "build"
    ],
    "maintainers":[
        {
            "name":"Ghislain Seguin",
            "email":"ghislain@gmail.com"
        }
    ],
    "contributors":[
        {
            "name":"Ghislain Seguin",
            "email":"ghislain@gmail.com"
        }
    ],
    "repositories":[
        {
            "type":"git",
            "url":"git@github.com:gseguin/node-amd-builder.git"
        }
    ],
    "dependencies":{
        "async":"latest",
        "css-concat":"latest",
        "express":"2.5.2",
        "mime":"latest",
        "node-promise":"latest",
        "request":"latest",
        "requirejs":">=1.0.7",
        "rimraf":"latest",
        "underscore":"latest",
        "node-native-zip":"~1.0.1"
    },
    "scripts":{
        "start":"REPO_BASE_DIR=/home/node/repositories WORK_BASE_DIR=/home/node/src node server.js"
    },
    "main":"server.js",
    "engine":{
        "node":"~0.6.11"
    }
}