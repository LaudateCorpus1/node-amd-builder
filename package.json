	"version": "0.1.2",
	"keywords": [ "jquery mobile", "amd", "build"],
	"maintainers": [{
		"name": "Ghislain Seguin",
		"email": "ghislain@gmail.com"
	}],
	"contributors": [{
		"name": "Ghislain Seguin",
		"email": "ghislain@gmail.com"
	}],
	"repositories": [{
		"type": "git",
		"url": "node@amd-builder.no.de:repo"
	}],
	"dependencies": {
        "async": "latest",
        "css-concat": "latest",
